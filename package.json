    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.0",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-native": "^0.63.4",
